<?xml version="1.0"?>
<options>
	<option>
		<short>rm</short>
		<long>conn.site.rm</long>
		<description>Full URL to RRC
			application. Example URL=https://jazz.net:9443/sandbox01/rm
		</description>
		<hasArgs>true</hasArgs>
		<argName>URL</argName>
		<required>true</required>
	</option>
	<option>
		<short>jts</short>
		<long>conn.site.jts</long>
		<description>Full URL to JTS
			application. Example URL=https://jazz.net:9443/sandbox01/jts
		</description>
		<hasArgs>true</hasArgs>
		<argName>URL</argName>
		<required>true</required>
	</option>
	<option>
		<short>u</short>
		<long>conn.user</long>
		<description>Registered user name.</description>
		<hasArgs>true</hasArgs>
		<argName>name</argName>
		<required>true</required>
	</option>
	<option>
		<short>p</short>
		<long>conn.password</long>
		<description>Password for registered user.</description>
		<hasArgs>true</hasArgs>
		<argName>pswd</argName>
		<required>true</required>
	</option>
	<option>
		<short>pr</short>
		<long>conn.projectArea</long>
		<description>Project Area title, if contains white-spaces put in quotes "prj XX".</description>
		<hasArgs>true</hasArgs>
		<argName>PrjArea</argName>
		<required>true</required>
	</option>
	<option>
		<short>h</short>
		<long>app.help</long>
		<description>Display usage help.</description>
		<hasArgs>false</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>v</short>
		<long>app.version</long>
		<description>Display current version.</description>
		<hasArgs>false</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>nl</short>
		<long>app.noLogo</long>
		<description>Do not display logo</description>
		<hasArgs>false</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>f</short>
		<long>optn.file</long>
		<description>Comma spareated Options files, located in [.|config|../config|absolute path]. ini file format [option=value]. See sample in config.ini</description>
		<hasArgs>true</hasArgs>
		<required>false</required>
		<argName>f1.opt,f2.opt</argName>
	</option>
	<option>
		<short>ad</short>
		<long>app.do</long>
		<description>Action to do on record. One of the following [create|read|update|delete]</description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>at</short>
		<long>app.reqType</long>
		<description>Requirement type of record (Feature, Use Case, System...). </description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>

	<option>
		<short>qt</short>
		<long>req.title</long>
		<description>Requirement title text.</description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>qc</short>
		<long>req.content</long>
		<description>Requirement content text. May contain XHTML tags</description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>qd</short>
		<long>req.desc</long>
		<description>Requirement description text.</description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>qfu</short>
		<long>req.folder.URI</long>
		<description>Requirement's folder URI.</description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>
	<option>
		<short>qa</short>
		<long>req.attr</long>
		<description>Requirement's attribute. Multi occurring, name=value. Examples priority=low, development stage=plan phase </description>
		<hasArgs>true</hasArgs>
		<required>false</required>
	</option>
	
</options>